<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.Administrator.loginservlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
    	<servlet-name>SparepartInsert</servlet-name>
    	<servlet-class>com.Administrator.SparepartInsert</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>SparepartInsert</servlet-name>
    	<url-pattern>/insert</url-pattern>
    </servlet-mapping>
    
    
    <servlet>
    	<servlet-name>updatesparepart</servlet-name>
    	<servlet-class>com.Administrator.updatesparepart</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>updatesparepart</servlet-name>
    	<url-pattern>/update</url-pattern>
    </servlet-mapping>
    
    <servlet>
    	<servlet-name>deletesparepart</servlet-name>
    	<servlet-class>com.Administrator.deletesparepart</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>deletesparepart</servlet-name>
    	<url-pattern>/delete</url-pattern>
    </servlet-mapping>
    
    <servlet>
    	<servlet-name>Administratorloginservlet</servlet-name>
    	<servlet-class>com.Administrator.Administratorloginservlet</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>Administratorloginservlet</servlet-name>
    	<url-pattern>/log</url-pattern>
    </servlet-mapping>
    
    
	
    
	
	</web-app>